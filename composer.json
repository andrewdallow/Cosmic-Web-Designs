{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "^3.3",
		"silverstripe/framework": "^3.3",
		"silverstripe/reports": "^3.3",
		"silverstripe/siteconfig": "^3.3",
		"silverstripe-themes/simple": "3.1.*",
		"silverstripe/googlesitemaps": "^1.5",
		"silverstripe/blog": "^2.4",
		"silverstripe/widgets": "^1.2",
		"silverstripe/comments": "^2.1",
		"gdmedia/ss-auto-git-ignore": "^1.0"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.3.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
        "scripts": {
            "post-update-cmd": "GDM\\SSAutoGitIgnore\\UpdateScript::Go"
       } 
}
